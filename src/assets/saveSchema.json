{
  "$schema": "http://json-schema.org/schema#",
  "$id": "saveSchema.json",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "hidden": {
        "type": "boolean"
      },
      "images": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "hidden": {
              "type": "boolean"
            },
            "base64data": {
              "type": "string"
            },
            "canvasData": {
              "type": "string"
            },
            "hotspots": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "strokeColor": {
                    "type": "string"
                  },
                  "base64sound": {
                    "type": "string"
                  },
                  "svgPointArray": {
                    "type": "array"
                  },
                  "typeSound": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    },
    "required": ["name", "hidden", "images"]
  }
}


